{
    "version": 2,
    "name": "user-api",
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/admin/(.*)",
            "dest": "src/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "src/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "regions": ["bom1"]
}